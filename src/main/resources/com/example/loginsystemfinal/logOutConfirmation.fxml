<?xml version="1.0" encoding="UTF-8"?>

<?import io.github.palexdev.materialfx.controls.MFXButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="confirmartionPane" prefHeight="148.0" prefWidth="276.0" style="-fx-border-color: lightgray; -fx-border-width: 1px; -fx-border-radius: 4px;" stylesheets="@css/styles.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.loginsystemfinal.LogOutConfirmation">
   <children>
      <Pane layoutY="1.0" prefHeight="147.0" prefWidth="278.0" stylesheets="@css/styles.css">
         <children>
            <Text layoutY="50.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text" text="Are you sure you want to logout?" textAlignment="CENTER" wrappingWidth="278.0">
               <font>
                  <Font name="Roboto Light" size="15.0" />
               </font>
            </Text>
            <MFXButton fx:id="ok" buttonType="RAISED" layoutX="45.0" layoutY="74.0" onAction="#gotoLoginPage" prefHeight="35.0" prefWidth="82.0" style="-fx-background-color: #2a313a; -fx-text-fill: white; -fx-font-family: Open Sans Regular;" text="Confirm" textFill="WHITE" />
            <MFXButton fx:id="cancel" buttonType="RAISED" layoutX="152.0" layoutY="74.0" onAction="#close" prefHeight="35.0" prefWidth="82.0" style="-fx-background-color: #2a313a; -fx-text-fill: white; -fx-font-family: Open Sans Regular;" text="Cancel" textFill="WHITE" />
         </children>
      </Pane>
   </children>
</AnchorPane>
